{
  "name": "todo-list",
  "version": "1.0.0",
  "description": "Todo List App built with SAP UI5 and TypeScript",
  "scripts": {
    "dev": "concurrently \"npm run build:watch\" \"npm start\" --names \"TypeScript,UI5\" --prefix-colors \"cyan,green\"",
    "start": "ui5 serve -o index.html",
    "lint": "eslint webapp/",
    "lint:fix": "eslint webapp/ --fix",
    "prettier": "prettier --check webapp/",
    "prettier:fix": "prettier --write webapp/",
    "build": "ui5 build --all --clean-dest",
    "build:ts": "tsc",
    "build:watch": "tsc --watch"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload"
    ]
  },
  "devDependencies": {
    "@eslint/config-helpers": "^0.3.0",
    "@eslint/js": "^9.30.1",
    "@prettier/plugin-xml": "^3.4.1",
    "@types/openui5": "^1.138.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@ui5/cli": "^4.0.19",
    "concurrently": "^9.1.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "ui5-middleware-livereload": "^3.1.2",
    "ui5-tooling-transpile": "^3.9.0"
  }
}
